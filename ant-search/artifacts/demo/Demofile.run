demo = 'dm/ant-colony-search'

########################################################
#
# 'demo run'
#
########################################################

[run]
default = "djibouti"

  [run.preconfigured.djibouti]
  only_log = '^best so far|^ Best Solution'
  script = '/demo/run/djibouti.sh'
  output_files = [
    'best.dj38',
    'cmp.dj38',
    'stat.dj38',
  ]

  [run.preconfigured.qatar]
  only_log = '^best so far|^ Best Solution'
  script = '/demo/run/qatar.sh'
  output_files = [
    'best.qa194',
    'cmp.qa194',
    'stat.qa194',
  ]

  [run.preconfigured.luxembourg]
  only_log = '^best so far|^ Best Solution'
  script = '/demo/run/luxembourg.sh'
  output_files = [
    'best.lu980',
    'cmp.lu980',
    'stat.lu980',
  ]
